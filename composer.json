{
    "name": "blip/bootstrap-form",
    "description": "Laravel 9 form wrappers for Bootstrap 5.",
    "keywords": [
        "laravel",
        "form",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [{
        "name": "linuxitos",
        "email": "contact@linuxitos.com"
    }],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/config": "~8.0|^9.0",
        "illuminate/session": "~8.0|^9.0",
        "illuminate/support": "~8.0|^9.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.4.1",
        "phpunit/phpunit": "~9.5"
    },
    "autoload": {
        "psr-4": {
            "Blip\\BootstrapForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Blip\\BootstrapForm\\BootstrapFormServiceProvider"
            ],
            "aliases": {
                "Blip": "Watson\\BootstrapForm\\Facades\\BootstrapForm"
            }
        }
    }
}
